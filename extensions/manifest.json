{
    "manifest_version": 2,
    "name": "sgtvpn-extension",
    "description": "sgtvpn-extension",
    "version": "0.0.1",
    "icons": {
      "16": "img/icon16.png",
      "32": "img/icon32.png",
      "48": "img/icon48.png",
      "128": "img/icon128.png",
      "512": "img/icon512.png"
    },
    "background": {
      "scripts": ["js/background.js"],
      "persistent": true
    },
    "browser_action": {
      "default_icon": {
        "19": "img/icon19.png",
        "38": "img/icon38.png"
      },
      "default_title": "SGT VPN"
    },
    "permissions": [
      "proxy",
    ]
}
